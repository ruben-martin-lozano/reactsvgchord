{
  "name": "ReactSvgChord",
  "version": "1.0.0",
  "description": "ReactSvgChord",
  "private": true,
  "main": "dist/",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "parcel build src/index.jsx --no-source-maps --out-dir dist",
    "build:css": "node-sass src/index.scss dist/index.css --output-style compressed",
    "clean": "rimraf dist",
    "removemap": "rimraf dist/*.map",
    "fenix": "rimraf node_modules && npm i -s",
    "start": "parcel demo/index.html --out-dir public --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "yay": "npm run clean && npm run build && npm run removemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ruben-martin-lozano@bitbucket.org/ruben-martin-lozano/reactsvgchord.git"
  },
  "keywords": [],
  "author": "Rubén Martín Lozano",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ruben-martin-lozano/reactsvgchord#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "sasslintConfig": ".sass-lint.yml",
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint-config-standard-react": "6.0.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "6.0.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint": "4.18.1",
    "node-sass": "4.7.2",
    "normalize.css": "8.0.0",
    "parcel-bundler": "1.7.0",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1"
  }
}
